    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
}
#form-comment textarea:hover {
    background:#fff;
}
#form-comment .personal-data {
    overflow: hidden;
}
#form-comment p {
    padding-bottom: 15px;
}
#form-comment .personal-data p {
    width: 50%;
}
.contactform label {
    display: block !important;
}
.txt, textarea {
    border: 1px solid #ccc;
    font-size: 13px;
    color: #444;
}
textarea {
    overflow: auto;
}
.loading {
    font-style: italic;
    display: none;
}
.errortxt, textarea.errortxt {
