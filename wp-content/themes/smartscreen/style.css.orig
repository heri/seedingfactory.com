}
/* Clients logo */
.clients a {
    display: block;
    float: left;
    margin: 3px;
}
.clients a img {
    float: left;
}
/* flickr widget */
.flickr .wrap {
    margin-left: 0;
    position: relative;
    padding: 10px 0 0;
}
.flickr a {
    display: inline-block;
    padding: 0 !important;
}
.flickr a img {
    border: 1px solid #777;
    float: left;
}
.flickr a:hover img {
    border: 1px solid #fff;
    transition: border 1s;
    -moz-transition: border 1s;
    -webkit-transition: border 1s;
    -o-transition: border 1s;
    -ms-transition: border 1s;
