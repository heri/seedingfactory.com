}
/* shutdown, menu toggle, page toggle, scrollup */
#shutdown, #menu-toggle, #entries-toggle, #scrollup {
    display: block;
    width: 32px;
    height: 32px;
    position: absolute;
    text-indent: -9999px;
    cursor: pointer;
}
