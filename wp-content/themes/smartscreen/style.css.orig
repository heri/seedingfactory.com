}
#nextslide {
    margin-right: 80px;
    background: url('images/slider/forward.png') no-repeat;
}
#play-button {
    margin-right: 5px;
}
#prevslide:hover, #nextslide:hover, #play-button:hover {
    cursor: pointer;
}
/* slideshow progress */
#progress-back {
    z-index: 5;
    position: fixed;
    bottom: 51px;
    left: 0;
    height: 4px;
    width: 100%;
    background: url('images/slider/progress-back.png') repeat-x;
}
